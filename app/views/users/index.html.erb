<%= render 'books/list', book: @book, user: current_user %>
<h1>Users</h1>
  <table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  </table>
<% @users.each do |user| %>
 <tbody>
   <tr>
     <td>
       <%= image_tag user.get_profile_image(100,100) %>
     </td>
     <td>
       <%= user.name %>
     </td>
     <td>
       <%= link_to 'show', user_path(user.id) %>
     </td>
   </tr>
 </tbody>
 
<% end %>