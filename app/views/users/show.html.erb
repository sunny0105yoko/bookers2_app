<main>
  <p id="notice">Welcome! You have signed up successfully.</p>
    <div class="container px-5 px-sm-0">
    <div class="row">
    <div class="col-md-3">

<%= render 'books/list', book: @book, user: @user %>
    
<!-- ユーザーの投稿一覧 -->
<h2>Books</h2>
<% @books.each do |book| %>
<div>
  <%= link_to book_path(book.id) do %>
  <% end %>
  <p><%= image_tag book.user.get_profile_image(100,100) %></p>
  <p>Tittle:<%= book.title %></p>
  <p>Opinion:<%= book.body %></p>
</div>
<% end %>